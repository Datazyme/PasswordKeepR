      <Span id="buttons">
        <button id="login-button">Login</button>
        <button id="register-button">register</button>
      </Span>
    </div>
  </header>

   <!-- Page-specific (main) content here -->

<div id="main" style="display: flex;">
  <div style="flex: 1;">
    <h3>Password Generator:</h3>
    <ul>
      <li><input type="checkbox" id="lowercase">Lowercase letters</li>
      <li><input type="checkbox" id="uppercase">Uppercase letters</li>
      <li><input type="checkbox" id="numbers">Numbers</li>
      <li><input type="checkbox" id="special">Special characters</li>
    </ul>
    <button>Submit</button>
  </div>
  <div style="flex: 1;">
    <h3>Generated Password:</h3>
    <textarea id="output" rows="5" disabled></textarea>
<button id="copy-button">Copy</button>
  </div>
</div>


<br>
